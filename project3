



#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>

int main()
{
	double a = 0;
	double b = 0;
	scanf_s("%lf,%lf", &a, &b);
	double c = 0;
	c = a * b;
	printf("乘积%lf",c);
	int e = 0;
	int f = 0;
	int g = 0;
	int h = 0;
	scanf_s("%d %d",&f,&e);
	
	g = e / f;
	h = e % f;
	printf("商%d",g);
	printf("余数%d", h);
    



	return 0;


}



